{
  "expo": {
    "name": "Propatiz",
    "slug": "propatiz",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/smaller.png",
    "userInterfaceStyle": "light",
    "scheme": "com.jaypkagimu.propatiz",
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/firstPage.png",
      "resizeMode": "contain",
      "backgroundColor": "#072c3e"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jaypkagimu.propatiz",
      "buildNumber": "1.0.0",
      "jsEngine": "jsc"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/smaller.png",
        "backgroundColor": "#072c3e"
      },
      "permissions": [
        "android.permission.CAMERA_ROLL",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.AUDIO_RECORDING",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "package": "com.jaypkagimu.propatiz",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/smaller.png"
    },
    "extra": {
      "eas": {
        "projectId": "9833f4de-3b37-4a0b-a089-26df87041109"
      }
    },
    
    "plugins": [
      "expo-image-picker",
      
      [
        "expo-av",
        {
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone."
        }
      ]
    ],
    "sdkVersion": "47.0.0"
  },
  "name": "propatiz"
}
