{
  "expo": {
    "name": "Propatiz",
    "slug": "Propatiz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/propatiz.jpg",
    "userInterfaceStyle": "light",
    "scheme": "com.jaypkagimu.landville",
    "splash": {
      "image": "./assets/firstpage.jpg",
      "resizeMode": "fill",
      "backgroundColor": "#072c3e"
    
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jaypkagimu.landville"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#212E52"
      },
      "permissions": [
      "CAMERA_ROLL",
      "READ_EXTERNAL_STORAGE",
      "WRITE_EXTERNAL_STORAGE",
      "AUDIO_RECORDING"
    ],
      "package": "com.jaypkagimu.landville"
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "d6146daf-2fa5-46bd-8dd0-60636c065145"
      }
    },
    "plugins": [
      "expo-image-picker",
      [
        "expo-av",
        {
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone."
        }
      ]
    ]
  }
}